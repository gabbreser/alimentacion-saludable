{
  "name": "saludable",
  "version": "0.1.0",
  "private": true,
  "homepage": "alimentacion-saludable/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "sh -ac '. ./.env; react-scripts start'",
    "build": "sh -ac '. ./.env; react-scripts build'",
    "build:ci": "sh -ac '. ./.env.CI; CI=false react-scripts build'",
    "build:nety": "sh -ac '. ./.env.NETY; CI=false react-scripts build'",
    "test": "CI=false react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --max-warnings=20",
    "lint:fix": "eslint . --max-warnings=20 --fix",
    "lint:eslint": "eslint . --cache --cache-location '../../eslintcache/' --format stylish",
    "lint2": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint2:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.8"
  }
}
